{
	"name": "obsidian-vault-explorer",
	"version": "1.41.0",
	"description": "Explore your vault in visual format",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "bun run check && bun run compile && node esbuild.config.mjs production",
		"guardgen": "ts-auto-guard",
