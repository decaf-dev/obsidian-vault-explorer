		"@types/lodash": "^4.17.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-svelte": "^0.8.0",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"svelte-check": "^3.8.4",
		"svelte-preprocess": "^5.1.4",
		"ts-auto-guard": "^5.0.0",
		"ts-jest": "^29.1.4",
